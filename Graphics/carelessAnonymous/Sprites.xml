<?xml version="1.0" encoding="utf-8" ?>
<Sprites> 

  <featherbumper path="objects/AnonHelper/featherBumper/" start="idle">
      <Center/>
      <Anim id="on" path="idle" frames="42-44" delay="0.06" goto="idle"/>
      <Loop id="idle" path="idle" frames="0-33" delay="0.06"/>
      <Anim id="hit" path="idle" frames="34-42" delay="0.06" goto="off"/>
      <Loop id="off" path="idle" frames="42" delay="0.06"/>
  </featherbumper>

  <whitecloud path="objects/AnonHelper/clouds/" start="spawn">
    <Justify x="0.5" y="0.5"/>
    <Loop id="whiteidle" path="whitecloud" delay="0.08" frames="0"/>
    <Loop id="whitedead" path="whitecloud" delay="0.08" frames="5"/>
    <Anim id="fade" path="whitecloud" delay="0.08" frames="0-4" goto="whitedead"/>
    <Anim id="spawn" path="whitecloud" delay="0.06" frames="6-10" goto="whiteidle"/>
  </whitecloud>
  
  <whitecloudRemix path="objects/AnonHelper/clouds/" start="whiteidle">
    <Justify x="0.5" y="0.5"/>
    <Loop id="whiteidle" path="whitecloudRemix" delay="0.08" frames="0"/>
    <Loop id="whitedead" path="whitecloudRemix" delay="0.08" frames="5"/>
    <Anim id="fade" path="whitecloudRemix" delay="0.08" frames="0-5" goto="whitedead"/>
    <Anim id="spawn" path="whitecloudRemix" delay="0.06" frames="0"/>
  </whitecloudRemix>

  <pinkcloud path="objects/AnonHelper/clouds/" start="pinkidle">
    <Justify x="0.5" y="0.5"/>
    <Loop id="pinkidle" path="pinkcloud" delay="0.08" frames="0"/>
    <Loop id="pinkdead" path="pinkcloud" delay="0.08" frames="5"/>
    <Anim id="fade" path="pinkcloud" delay="0.08" frames="0-5" goto="pinkdead"/>
    <Anim id="spawn" path="pinkcloud" delay="0.08" frames="0"/>
  </pinkcloud>

  <pinkcloudRemix path="objects/AnonHelper/clouds/" start="pinkidle">
    <Justify x="0.5" y="0.5"/>
    <Loop id="pinkidle" path="pinkcloudRemix" delay="0.08" frames="0"/>
    <Loop id="pinkdead" path="pinkcloudRemix" delay="0.08" frames="5"/>
    <Anim id="fade" path="pinkcloudRemix" delay="0.08" frames="0-5" goto="pinkdead"/>
    <Anim id="spawn" path="pinkcloudRemix" delay="0.06" frames="0"/>
  </pinkcloudRemix>

  <windcloud path="objects/AnonHelper/clouds/" start="windidle">
    <Justify x="0.5" y="0.5"/>
    <Loop id="windidle" path="windcloud" delay="0.08" frames="0"/>
    <Anim id="fade" path="windcloud" delay="0.08" frames="1-5"/>
    <Anim id="spawn" path="windcloud" delay="0.06" frames="0"/>
  </windcloud>

  <windcloudRemix path="objects/AnonHelper/clouds/" start="windidle">
    <Justify x="0.5" y="0.5"/>
    <Loop id="windidle" path="windcloudRemix" delay="0.08" frames="0"/>  
    <Anim id="fade" path="windcloudRemix" delay="0.08" frames="1-5"/>
    <Anim id="spawn" path="windcloudRemix" delay="0.06" frames="0"/>
  </windcloudRemix>

  <windfragile path="objects/AnonHelper/clouds/" start="windfragileidle">
    <Justify x="0.5" y="0.5"/>
    <Loop id="windfragileidle" path="windfragile" delay="0.08" frames="0"/>
    <Loop id="windfragiledead" path="windfragile" delay="0.08" frames="5"/>
    <Anim id="fade" path="windfragile" delay="0.08" frames="0-5" goto="windfragiledead"/>
    <Anim id="spawn" path="windfragile" delay="0.06" frames="0"/>
  </windfragile>

  <windfragileRemix path="objects/AnonHelper/clouds/" start="windfragileidle">
    <Justify x="0.5" y="0.5"/>
    <Loop id="windfragileidle" path="windfragileRemix" delay="0.08" frames="0"/>
