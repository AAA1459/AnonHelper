    <Anim id="spawn" path="windfragileRemix" delay="0.06" frames="0"/>
  </windfragileRemix>

  <bounceCenter path="objects/AnonHelper/bumpBlock/" start="hot">
    <Center/>
    <Anim id="hotWait" path="center" frames="0-7" delay="0.08" goto="hotWaitLoop"/>
    <Loop id="hotWaitLoop" path="center" frames="4-7" delay="0.08"/>
    <Loop id="hot" path="center" frames="8-12" delay="0.08"/>
    <Anim id="iceWait" path="center" frames="19-25" delay="0.08" goto="iceWaitLoop"/>
    <Loop id="iceWaitLoop" path="center" frames="24-25" delay="0.08"/>
    <Loop id="ice" path="center" frames="13-18" delay="0.08"/>
  </bounceCenter>

  <bumpBlockCenterIce path="objects/AnonHelper/bumpBlockNew/" start="idle">
    <Center/>
    <Loop id="idle" path="ice_center" delay="0.1"/>
  </bumpBlockCenterIce>
  
  <bumpBlockCenterFire path="objects/AnonHelper/bumpBlockNew/" start="idle">
    <Center/>
    <Loop id="idle" path="fire_center" delay="0.08"/>
  </bumpBlockCenterFire>

</Sprites>